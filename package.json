{
  "name": "omn",
  "version": "1.0.0",
  "description": "A practical, lightweight and unique JavaScript toolkit.",
  "author": "chiskat <1@paperplane.cc> (https://paperplane.cc)",
  "license": "MIT",
  "homepage": "https://omn.paperplane.cc",
  "repository": {
    "type": "git",
    "url": "https://github.com/chiskat/omn"
  },
  "bugs": {
    "url": "https://github.com/chiskat/omn/issues"
  },
  "keywords": [
    "util",
    "utils",
    "utilities",
    "toolkit",
    "helper",
    "helpers",
    "functions"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "build": "rollup -c",
    "format": "prettier --list-different .",
    "lint": "eslint",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.11",
    "@vitest/browser-playwright": "^4.0.18",
    "commitizen": "^4.3.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "^9",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.4.3",
    "lodash-es": "^4.17.21",
    "playwright": "^1.58.2",
    "prettier": "^3.5.3",
    "rollup": "^4.35.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.39.1",
    "vitepress": "1.6.4",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  }
}
